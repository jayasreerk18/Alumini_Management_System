class alumni:
def __init__(self, aname, areg, dob, batchnumber, emailid):
self.aname = aname
self.areg = areg
self.dob = dob
self.batchnumber = batchnumber
self.emailid = emailid
def accept(self, aname, areg, dob, batchnumber, emailid):
list = alumni(self, aname, areg, dob, batchnumber, emailid)
list.append(list)
